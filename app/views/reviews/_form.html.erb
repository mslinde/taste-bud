<div class="container-contact100">

    <%= simple_form_for([@event, @review]) do |f| %>
      <div class="form-box">
        <div class="left">
          <%= f.input :venue, collection: 1..5 %>
          <%= f.input :vibe, collection: 1..5%>
          <%= f.input :neighborhood, collection: 1..5 %>
          <%= f.input :comment, as: :text %>
        </div>
        </div>
      </div>
      <div class="button-div">
        <%= f.submit class: "btn-submit" %>
      </div>
  <% end %>
</div>



  <h3>Comments</h3>
  <div class="comments-container">
    <%= form_with model: [@spot_occupied, @comment], remote: true do |f| %>
        <div id="comments-form"><%= f.text_field :note, label: "Leave a comment for tonight's event", as: :text %></div>
        <%= f.submit "Post", class: "btn-dark-transparent"%>
    <% end %>
  </div>

